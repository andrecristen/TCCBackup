<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.andrecristen.tccperguntasseparadas.Conteudos.escolherConteudos">

    <ScrollView
        android:layout_width="370dp"
        android:layout_height="373dp"
        android:id="@+id/scrollView2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:ignore="UselessLeaf">

            <ImageButton
                android:id="@+id/imageButton11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="startTelaAdicao"
                app:srcCompat="@drawable/adicao"
                tools:ignore="ContentDescription" />

            <ImageButton
                android:id="@+id/imageButton12"
                android:onClick="startTelaSubtracao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/subtracao"
                tools:ignore="ContentDescription" />

            <ImageButton
                android:id="@+id/imageButton13"
                android:onClick="startTelaMultiplicacao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/multiplicacao"
                tools:ignore="ContentDescription" />

            <ImageButton
                android:id="@+id/imageButton14"
                android:onClick="startTelaDivisao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/divisao"
                tools:ignore="ContentDescription" />

            <ImageButton
                android:id="@+id/imageButton15"
                android:onClick="startTelaComplementos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/complementos"
                tools:ignore="ContentDescription" />

        </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="217dp"
        android:layout_height="173dp"
        app:srcCompat="@drawable/telaconteudos"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintVertical_bias="0.0"
        android:contentDescription="" />
</android.support.constraint.ConstraintLayout>
